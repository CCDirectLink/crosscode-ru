{
  "private": true,
  "name": "crosscode-ru-ng-translation-tool",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Russian translation tool for CrossCode",
  "homepage": "https://github.com/dmitmel/crosscode-ru-ng",
  "engines": {
    "node": ">=11.0.0"
  },
  "scripts": {
    "build": "node \"$npm_execpath\" run build:scripts && node \"$npm_execpath\" run build:styles",
    "build:scripts": "../node_modules/.bin/tsc",
    "watch:scripts": "../node_modules/.bin/tsc --watch",
    "build:styles": "node-sass --recursive --output dist/styles --source-map true src/styles",
    "watch:styles": "node-sass --recursive --output dist/styles --source-map true src/styles --watch"
  },
  "module": true,
  "prestart": "dist/mod/prestart.js",
  "ccmodDependencies": {
    "crosscode-ru-ng": "*"
  },
  "devDependencies": {
    "@types/node": "^13.7.0",
    "@types/nw.js": "^0.13.9",
    "node-sass": "^4.13.1"
  }
}
